{
  "name": "innards",
  "version": "0.0.1",
  "description": "Bones and guts for a site",
  "repository": "ghostmechanics/innards",
  "main": "server.js",
  "dependencies": {
    "blankie": "2.0.0",
    "glue": "4.0.0",
    "good": "7.1.0",
    "good-console": "6.2.0",
    "good-squeeze": "5.0.1",
    "hapi": "16.1.0",
    "hapi-pino": "1.3.0",
    "inert": "4.1.0",
    "normalize-styl": "4.1.1",
    "scooter": "4.0.0",
    "vision": "4.1.1"
  },
  "devDependencies": {
    "devtool": "2.3.1",
    "eslint": "3.12.2",
    "jeet": "7.1.0",
    "nib": "1.1.2",
    "pug": "2.0.0-beta6",
    "rupture": "0.6.2",
    "stylus": "0.54.5",
    "webpack": "2.2.0-beta11"
  },
  "scripts": {
    "build": "npm run build-css && npm run build-js",
    "build-css": "stylus -c -u ./node_modules/nib -u ./node_modules/jeet -u ./node_modules/rupture css/styles.styl",
    "build-js": "webpack --progress --watch",
    "debug": "./node_modules/.bin/devtool server.js",
    "lint": "./node_modules/.bin/eslint ./js --cache",
    "start": "npm run start-server && npm run build",
    "start-server": "node ./server.js"
  },
  "author": "John Michel <johnmichel@gmail.com>",
  "license": "MIT"
}